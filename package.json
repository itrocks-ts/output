	"keywords": [
		"backend",
		"component",
		"output",
		"it.rocks",
		"UI",
		"UX"
	],
	"license": "ISC",
	"name": "@itrocks/output",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/itrocks-ts/output.git"
